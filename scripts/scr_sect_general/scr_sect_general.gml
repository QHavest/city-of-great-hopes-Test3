function scr_sect_general(){
val1 = irandom_range(0,13);
switch(val1){
case 0:
instance_create_depth(595,hresty,-300,obj_helps_sec);
break;
case 1:
instance_create_depth(700,hresty,-300,obj_helps_sec);
break;
case 2:
instance_create_depth(805,hresty,-300,obj_helps_sec);
break;
case 3:
instance_create_depth(910,hresty,-300,obj_helps_sec);
break;
case 4:
instance_create_depth(1015,hresty,-300,obj_helps_sec);
break;
case 5:
instance_create_depth(1120,hresty,-300,obj_helps_sec);
break;
case 6:
instance_create_depth(1225,hresty,-300,obj_helps_sec);
